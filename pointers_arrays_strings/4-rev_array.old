#include "main.h"
#include <stdio.h>

/**
 * reverse_array - print an array of integers
 * @a: an array of integers
 * @n: the number of elements to swap
 *
 * Return: nothing.
 */

void reverse_array(int *a, int n)
{
	int i;
	int misize;
	int size;
	
	size = n;
	misize = n / 2;
	for (i = 0; size > misize; i++)
	{
			a[i] = a[size-1];
			size--;
	}
	for (; misize >= 0; misize--)
	{
		a[i] = a[misize];
	}
}	
	
